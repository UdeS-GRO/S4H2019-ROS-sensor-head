<?xml version="1.0"?>
<launch>
  <arg name="joy_dev" default="/dev/input/js2" />

  <node pkg="joy" type="joy_node" name="joy_node" output="log" respawn="false" respawn_delay="0">
    <param name="dev" value="$(arg joy_dev)" />
  </node>
  <node pkg="sensor_head_gui" name="main_node" type="node_logic" respawn="false" output="screen" />

  <node pkg="sensor_head_gui" type="manual_control.py" name="manual_control" output="screen" respawn="false" respawn_delay="0">
    
  </node>
  
  <node pkg="sensor_head_gui" type="main_control.py" name="main_control_node" output="screen" respawn="false" respawn_delay="0">
    
  </node>
  <include file="$(find sensor_head_gui)/launch/sensor_head_gui.launch">
    
  </include>

</launch>
